//if statement -> global JS

ggbObject.registerClientListener(updateEscTextCount);

function updateEscTextCount(a) {
  if (a.type == "select") {
    if (a.target == "AAppletStatus") {
      ggbObject.setValue("escTextCount", ggbObject.getValue("escTestCount") + 1);
    }
  }
}

//switch case -> global JS
function focusIndicatorsAndErrorMessages(a) {
  switch (a.type) {
    case "select":
      switch (a.target) {
        case "AAppletStatus":
          ggbObject.setValue("escTextCount", ggbObject.getValue("escTextCount") + 1);
          break;
      }
  }
}

//if statement platform
ggb1.instance.registerClientListener(updateEscTextCount);

function updateEscTextCount(a) {
  if (a.type == "select") {
    if (a.target == "AAppletStatus") {
      ggb1.instance.setValue("escTextCount", ggb1.instance.getValue("escTestCount") + 1);
    }
  }
}

//switch case -> platform
function focusIndicatorsAndErrorMessages(a) {
  switch (a.type) {
    case "select":
      switch (a.target) {
        case "AAppletStatus":
          ggb1.instance.setValue("escTextCount", ggb1.instance.getValue("escTextCount") + 1);
          break;
      }
  }
}
